{
    "name": "ralphjsmit/laravel-glide",
    "description": "Auto-magically generate responsive images from static image files.",
    "keywords": [
        "ralphjsmit",
        "laravel",
        "laravel-glide"
    ],
    "homepage": "https://github.com/ralphjsmit/laravel-glide",
    "license": "MIT",
    "authors": [
        {
            "name": "Ralph J. Smit",
            "email": "rjs@ralphjsmit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.29|^11.0|^12.0",
        "illuminate/support": "^10.29|^11.0|^12.0",
        "illuminate/view": "^12.2",
        "league/glide-symfony": "^2.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "RalphJSmit\\Laravel\\Glide\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RalphJSmit\\Laravel\\Glide\\GlideServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
